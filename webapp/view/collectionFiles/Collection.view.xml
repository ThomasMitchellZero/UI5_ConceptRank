<mvc:View 
  controllerName="TM.ConceptRank.controller.collectionFiles.Collections" 
	xmlns:mvc="sap.ui.core.mvc" 
	displayBlock="true" 
	xmlns="sap.m"
  xmlns:l="sap.ui.layout"
  xmlns:f="sap.ui.layout.form"
	xmlns:grid="sap.ui.layout.cssgrid"
	xmlns:core="sap.ui.core">
	
	
    <Page
      title="Create New Concept"
      showNavButton="true"
		  navButtonPress="onNavBack">

      <Button
        text="List O' Collections">

      </Button>

      <Panel>

        <content>
          <f:SimpleForm
            columnsL="1"
            columnsM="1">
              <f:content>

                <Label text="Collection Title"/>
                <Text text="{collectionTitle}"/>

              </f:content>

          </f:SimpleForm>


        </content>


      </Panel>


    </Page>

</mvc:View>