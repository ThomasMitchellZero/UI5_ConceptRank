<mvc:View 
	controllerName="TM.ConceptRank.controller.Home" 
	xmlns:mvc="sap.ui.core.mvc" 
	displayBlock="true" 
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:grid="sap.ui.layout.cssgrid"
	xmlns:core="sap.ui.core">
	

  <Page id="page" title="{i18n>title}">
    <content>

          <Button
            id="addNewCollectionBtn"
            text="Add New Concept Collection"
            press="onCreateNew">
          </Button>
          <Button
            id="testButton"
            text="Test Button"
            press="onDisplayNotFound">
          </Button>

          <List
            items="{primary_product_data>/conceptCollectionList}">
            <items>
              <ObjectListItem
                title="{primary_product_data>collectionMeta/collectionName}"
                icon="sap-icon://background"
                type="Navigation"
                press="pressedListItem">
                
              </ObjectListItem>
          </items>
          </List>


    </content>
  </Page>
</mvc:View>