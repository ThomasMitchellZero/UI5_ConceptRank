{
	"_version": "1.12.0",
	"sap.app": {
		"id": "TM.ConceptRank",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "TM.ConceptRank.view.Main",
			"type": "XML",
			"async": true,
			"id": "Main"
		},
		"dependencies": {
			"minUI5Version": "1.60.1",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "TM.ConceptRank.i18n.i18n"
				}
			},
			"primary_product_data":{
				"type":"sap.ui.model.json.JSONModel",
				"uri":"model/PrimaryProductData.json"
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "TM.ConceptRank.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false,
				"bypassed":{
					"target": "notFound"
				}
			},
			"routes": [{
				"pattern": "",
				"name": "NameHome",
				"target": ["targethome"]
			},{
				"pattern":"addnewcollection",
				"name":"AddNewCollection",
				"target":["targetAddNewColl"]
			},{
				"pattern":"seecollections",
				"name": "SeeCollections",
				"target": "targetSeeCollections"
			},{
				"pattern":"/{collections}",
				"name":"collection",
				"target":["targetCollections"]

			}
		
		],
			"targets": {
				"targethome": {
					"viewName": "Home",
					"viewLevel": 1
				},
				"notFound":{
					"viewName":"NotFound",
					"transition":"show"
				},
				"targetAddNewColl":{
					"viewName":"AddNewColl",
					"viewLevel": 2
				},
				"targetSeeCollections":{
					"viewName": "SeeCollections",
					"viewLevel": 2
				},
				"targetCollection":{
					"viewName":"collection.Collection",
					"viewLevel":3
				}

			}
		}
	}
}